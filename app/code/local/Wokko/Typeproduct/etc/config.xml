<?xml version="1.0" encoding="UTF-8"?>
<config>
	<modules>
		<Wokko_Typeproduct>
			<version>0.1.0</version>
		</Wokko_Typeproduct>
	</modules>
	<global>
		<models>
			<typeproduct>
				<class>Wokko_Typeproduct_Model</class>
			</typeproduct>
		</models>
		<helpers>
			<typeproduct>
				<class>Wokko_Typeproduct_Helper</class>
			</typeproduct>
		</helpers>
		<blocks>
			<typeproduct>
				<class>Wokko_Typeproduct_Block</class>
			</typeproduct>
		</blocks>
		<catalog>
			<product>
				<type>
					<typeproduct translate="label" module="typeproduct">
					<label>My Product Type</label>
					<model>typeproduct/product_type</model>
					<price_model>typeproduct/product_price</price_model>
					<composite>0</composite>
					<is_qty>1</is_qty>
					</typeproduct>
				</type>
			</product>
		</catalog>
		<resources>
			<typeproduct_setup>
				<setup>
					<module>Wokko_Typeproduct</module>
					<class>Wokko_Typeproduct_Model_Resource_Mysql4_Setup</class>
				</setup>
				<connection>
					<use>core_setup</use>
				</connection>
			</typeproduct_setup>
		</resources>
	</global>
	<adminhtml>
		<translate>
			<modules>
				<Wokko_Typeproduct>
					<file>
						<default>Wokko_Typeproduct.csv</default>
					</file>
				</Wokko_Typeproduct>
			</modules>
		</translate>
	</adminhtml>
	<frontend>
		<layout>
			<updates>
				<typeproduct>
					<file>typeproduct.xml</file>
				</typeproduct>
			</updates>
		</layout>
	</frontend>
</config>